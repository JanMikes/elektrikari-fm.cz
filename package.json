{
    "name": "elektrikari-fm",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "watch:sass": "sass --watch --quiet-deps --style compressed --source-map --embed-sources --no-error-css --load-path=node_modules src/styles:_site/css",
        "watch:start": "eleventy --serve",
        "build:sass": "sass --quiet-deps --source-map --embed-sources --no-error-css --load-path=node_modules src/styles:_site/css",
        "build:eleventy": "eleventy",
        "start": "npm-run-all --parallel watch:*",
        "build": "npm-run-all --parallel build:*"
    },
    "devDependencies": {
        "@11ty/eleventy": "^3.0",
        "@webpixels/css": "^2.0.0-beta.19",
        "bootstrap": "^5.3.2",
        "npm-run-all": "^4.1.5",
        "sass": "^1.81.0"
    }
}
